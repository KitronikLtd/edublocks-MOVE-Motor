<category name=":MOVE Motor" colour="#02AD4D" iconclass="blocklyTreeIconCustom robot">
  <label text="Kitronik :MOVE Motor" web-class="myLabelStyle"></label>
  <block type="import_move_motor"></block>

  <block type="move_motor_init"></block>

  <block type="move_motor_on">
    <value name="motor_speed">
      <shadow type="move_motor_speed"></shadow>
    </value>
  </block>

  <block type="move_motor_off"></block>

  <block type="move_motor_position">
    <value name="servo_angle">
      <shadow type="move_motor_servo_angle"></shadow>
    </value>
  </block>

  <block type="move_motor_set_led">
    <value name="parameters">
      <shadow type="parameters">
          <mutation color="#10b981" items="3" limit="3" minimum="1" inputType="number" />
      </shadow>
    </value>
  </block>
 
  <block type="move_motor_set_leds">
  </block>

  <block type="move_motor_show_leds"></block>
</category>
